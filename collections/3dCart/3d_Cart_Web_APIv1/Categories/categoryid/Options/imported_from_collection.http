#!/usr/bin/env dothttp

# imported from 35240-17a6f18b-38c5-42cb-a11e-28f755ffce28.postman_collection.json

// Get the options from a specific Category

@name("Get the options from a specific Category")
GET "https://{{baseUrl}}/3dCartWebAPI/v1/Categories/:categoryid/Options"
? "limit"= "<integer>"
? "offset"= "<integer>"


@name("Updates specific options from a specific Category")
PUT "https://{{baseUrl}}/3dCartWebAPI/v1/Categories/:categoryid/Options/:optionsetid"



