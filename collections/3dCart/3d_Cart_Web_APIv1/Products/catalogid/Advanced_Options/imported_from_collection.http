#!/usr/bin/env dothttp

# imported from 35240-17a6f18b-38c5-42cb-a11e-28f755ffce28.postman_collection.json

@name("Get the advanced options from a specific Product")
GET "https://{{baseUrl}}/3dCartWebAPI/v1/Products/:catalogid/AdvancedOptions"
? "limit"= "<integer>"
? "offset"= "<integer>"


@name("Updates specific advanced options from a specific Product")
PUT "https://{{baseUrl}}/3dCartWebAPI/v1/Products/:catalogid/AdvancedOptions/:advancedoptioncode"



