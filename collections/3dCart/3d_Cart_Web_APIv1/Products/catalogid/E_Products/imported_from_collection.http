#!/usr/bin/env dothttp

# imported from 35240-17a6f18b-38c5-42cb-a11e-28f755ffce28.postman_collection.json

@name("Get the EProducts from a specific Product")
GET "https://{{baseUrl}}/3dCartWebAPI/v1/Products/:catalogid/EProducts"
? "limit"= "<integer>"
? "offset"= "<integer>"


@name("Updates a specific eproduct from a specific Product")
PUT "https://{{baseUrl}}/3dCartWebAPI/v1/Products/:catalogid/EProducts/:filenumber"



