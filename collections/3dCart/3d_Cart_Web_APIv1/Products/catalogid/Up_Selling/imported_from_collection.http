#!/usr/bin/env dothttp

# imported from 35240-17a6f18b-38c5-42cb-a11e-28f755ffce28.postman_collection.json

@name("Get the upselling products from a specific Product")
GET "https://{{baseUrl}}/3dCartWebAPI/v1/Products/:catalogid/UpSelling"
? "limit"= "<integer>"
? "offset"= "<integer>"


@name("Updates a specific Upselling Item from a specific Product")
PUT "https://{{baseUrl}}/3dCartWebAPI/v1/Products/:catalogid/UpSelling/:upsellingindexid"



