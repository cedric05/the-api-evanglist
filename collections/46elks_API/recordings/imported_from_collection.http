#!/usr/bin/env dothttp

# imported from 35240-77b9e26c-075d-429f-817d-a74ef109fb90.postman_collection.json

// Get list of recordings

@name("Get Recordings")
GET "https://{{baseUrl}}/recordings"
? "start"= "<string>"
? "end"= "<string>"


// Get metadata of one recording.

@name("Get Recordings")
GET "https://{{baseUrl}}/recordings/:id"


// Get recodring soundfile.

@name("Get Recordings")
GET "https://{{baseUrl}}/recordings/{{id}}.wav"



