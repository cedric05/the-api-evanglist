// The Triggers endpoint returns information about the user's triggers.

@name("All triggers for current user")
GET "https://{{baseUrl}}/:username/triggers"
? "No Name"= "<string>"


@name("Delete an existing Trigger")
DELETE "https://{{baseUrl}}/:username/triggers/:id"
? "No Name"= "<string>"



