// Delete a page by ID.

@name("Delete Blog Page")
DELETE "https://{{baseUrl}}/blogs/:blogId/pages/:pageId"


// Publishes a draft page.

@name("Publish Blog Page")
POST "https://{{baseUrl}}/blogs/:blogId/pages/:pageId/publish"


// Revert a published or scheduled page to draft state.

@name("Revert Blog Page")
POST "https://{{baseUrl}}/blogs/:blogId/pages/:pageId/revert"



