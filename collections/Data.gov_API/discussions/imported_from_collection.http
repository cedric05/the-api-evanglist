#!/usr/bin/env dothttp

# imported from 35240-bc9332c4-d7ba-4398-b9a9-51c6aaf5271c.postman_collection.json

// List all Discussions

@name("Get Discussions")
GET "https://{{baseUrl}}/discussions/"
? "sort"= "<>"
? "page_size"= "<string>"
? "page"= "<string>"
? "for"= "<>"
? "closed"= "<string>"


// Delete a discussion given its ID

@name("Delete Discussions")
DELETE "https://{{baseUrl}}/discussions/:id/"



