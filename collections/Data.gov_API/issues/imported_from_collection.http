// List all Issues

@name("Get Issues")
GET "https://{{baseUrl}}/issues/"
? "sort"= "<>"
? "page_size"= "<string>"
? "page"= "<string>"
? "for"= "<>"
? "closed"= "<string>"


// Get an issue given its ID

@name("Get Issues")
GET "https://{{baseUrl}}/issues/:id/"



