#!/usr/bin/env dothttp

# imported from 35240-a7f953fa-f8c8-4115-9ab9-d70490b97ce6.postman_collection.json

// Returns an index of acts.

@name("Act Index")
GET "https://{{battle-net-host}}/d3/data/act"
? "locale"= "{{locale}}"
? "access_token"= "{{access-token}}"


// Returns a single act by ID.

@name("Act")
GET "https://{{battle-net-host}}/d3/data/act/{{d3.act-id}}"
? "locale"= "{{locale}}"
? "access_token"= "{{access-token}}"



