#!/usr/bin/env dothttp

# imported from 35240-3abc9a78-733b-4c0c-8e67-86b81dc5a077.postman_collection.json

// Create an action on a collection.

@name("Create a collection action")
POST "https://{{baseUrl}}/collections/:collectionId/actions/:actionType"
? "No Name"= "<string>"


// Read an action on a collection by ID.

@name("Read a collection action")
GET "https://{{baseUrl}}/collections/:collectionId/actions/:actionType/:actionId"
? "No Name"= "<string>"



