#!/usr/bin/env dothttp

# imported from 35240-3abc9a78-733b-4c0c-8e67-86b81dc5a077.postman_collection.json

// Create an action on a product.

@name("Create a product action")
POST "https://{{baseUrl}}/products/:productId/actions/:actionType"
? "No Name"= "<string>"


// Read an action on a product by ID.

@name("Read a product action")
GET "https://{{baseUrl}}/products/:productId/actions/:actionType/:actionId"
? "No Name"= "<string>"



