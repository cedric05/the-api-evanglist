#!/usr/bin/env dothttp

# imported from 35240-3abc9a78-733b-4c0c-8e67-86b81dc5a077.postman_collection.json

// Create a new policy to apply a schema to a role.

@name("Create a policy")
POST "https://{{baseUrl}}/roles/:roleId/policies"
? "No Name"= "<string>"


// Read a single policy on a role by ID.

@name("Read a policy")
GET "https://{{baseUrl}}/roles/:roleId/policies/:policyId"
? "No Name"= "<string>"



