#!/usr/bin/env dothttp

# imported from 35240-3abc9a78-733b-4c0c-8e67-86b81dc5a077.postman_collection.json

// Create an action on a Thng.

@name("Create a Thng action")
POST "https://{{baseUrl}}/thngs/:thngId/actions/:actionType"
? "No Name"= "<string>"


// Read an action on a Thng by ID.

@name("Read a Thng action")
GET "https://{{baseUrl}}/thngs/:thngId/actions/:actionType/:actionId"
? "No Name"= "<string>"



