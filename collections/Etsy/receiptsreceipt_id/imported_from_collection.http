#!/usr/bin/env dothttp

# imported from 35240-3659b137-52c3-4941-8c35-36a1a993a79b.postman_collection.json

// Retrieves a Receipt by id.

@name("Get Receipts Receipt")
GET "https://{{baseUrl}}/receipts/:receipt_id"


// Retrieves a set of Transaction objects associated to a Receipt.

@name("Get Receipts Receipt Transactions")
GET "https://{{baseUrl}}/receipts/:receipt_id/transactions"
? "limit"= "<>"
? "offset"= "<>"



