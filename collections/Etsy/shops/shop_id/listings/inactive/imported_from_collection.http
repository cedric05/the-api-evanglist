#!/usr/bin/env dothttp

# imported from 35240-3659b137-52c3-4941-8c35-36a1a993a79b.postman_collection.json

// Retrieves Listings associated to a Shop that are inactive

@name("Get Shops Shop Listings Inactive")
GET "https://{{baseUrl}}/shops/:shop_id/listings/inactive"
? "limit"= "<>"
? "offset"= "<>"


// Retrieves a Listing associated to a Shop that is inactive

@name("Get Shops Shop Listings Inactive Listing")
GET "https://{{baseUrl}}/shops/:shop_id/listings/inactive/:listing_id"



