// Retrieves children of a top-level Category by tag.

@name("Get Taxonomy Categories Tag")
GET "https://{{baseUrl}}/taxonomy/categories/:tag"


// Retrieves children of a second-level Category by tag and subtag.

@name("Get Taxonomy Categories Tag Subtag")
GET "https://{{baseUrl}}/taxonomy/categories/:tag/:subtag"



