#!/usr/bin/env dothttp

# imported from 35240-3659b137-52c3-4941-8c35-36a1a993a79b.postman_collection.json

// Retrieves a set of UserAddress objects associated to a User.

@name("Get Users User Addresses")
GET "https://{{baseUrl}}/users/:user_id/addresses"
? "limit"= "<>"
? "offset"= "<>"


// Retrieves a UserAddress by id.

@name("Get Users User Addresses User Address")
GET "https://{{baseUrl}}/users/:user_id/addresses/:user_address_id"



