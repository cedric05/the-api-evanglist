#!/usr/bin/env dothttp

# imported from 35240-3b0d7f32-a990-42e1-8346-61957d7dfc30.postman_collection.json

// Gets a user's project list by user id.

@name("Gets a user's project list by user id")
GET "https://{{baseUrl}}/users/:userid/projects"
? "sortby"= "<>"
? "fetchHierarchyPath"= "<>"


// Adds a user to specified project's member list by user id.

@name("Adds a user to specified project's member list by user id")
PUT "https://{{baseUrl}}/users/:userid/projects/:projectid"



