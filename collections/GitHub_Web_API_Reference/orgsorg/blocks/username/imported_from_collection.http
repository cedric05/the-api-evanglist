@name("Check if a user is blocked by an organization")
GET "https://{{baseUrl}}/orgs/:org/blocks/:username"


@name("Block a user from an organization")
PUT "https://{{baseUrl}}/orgs/:org/blocks/:username"


@name("Unblock a user from an organization")
DELETE "https://{{baseUrl}}/orgs/:org/blocks/:username"



