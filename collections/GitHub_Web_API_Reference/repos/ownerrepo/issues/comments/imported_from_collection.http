#!/usr/bin/env dothttp

# imported from 35240-c446a4c9-8dd6-45c4-99f2-688a96fe76ae.postman_collection.json

// By default, Issue Comments are ordered by ascending ID.

@name("List issue comments for a repository")
GET "https://{{baseUrl}}/repos/:owner/:repo/issues/comments"
? "sort"= "created"
? "direction"= "<string>"
? "since"= "<string>"
? "per_page"= "30"
? "page"= "1"



