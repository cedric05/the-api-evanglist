#!/usr/bin/env dothttp

# imported from 35240-cc491087-45e6-46ba-aa93-9f2f2cc44068.postman_collection.json

// Gets a list of group or project members viewable by the authenticated user.

@name("Get Groups  Members")
GET "https://{{baseUrl}}/groups/:id/members"
? "query"= "<>"
? "page"= "<>"
? "per_page"= "<>"


// Removes a user from a group or project.

@name("Delete Groups  Members User")
DELETE "https://{{baseUrl}}/groups/:id/members/:user_id"



