#!/usr/bin/env dothttp

# imported from 35240-78c6df8c-aca6-4ba0-aa1b-721d91b41fef.postman_collection.json

// Retrieve all of the existing custom fields for a product.

@name("Get Products Custom Fields")
GET "https://{{baseUrl}}/products/:product_id/custom_fields"


// Edit an existing products custom field.

@name("Put Products Custom Fields Name")
PUT "https://{{baseUrl}}/products/:product_id/custom_fields/:name"
? "required"= "<>"
? "variant"= "<>"



