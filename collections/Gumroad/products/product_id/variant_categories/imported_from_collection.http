#!/usr/bin/env dothttp

# imported from 35240-78c6df8c-aca6-4ba0-aa1b-721d91b41fef.postman_collection.json

// Retrieve all of the existing variant categories of a product.

@name("Get Products Variant Categories")
GET "https://{{baseUrl}}/products/:product_id/variant_categories"


// Permanently delete a variant category of a product.

@name("Delete Products Variant Categories")
DELETE "https://{{baseUrl}}/products/:product_id/variant_categories/:id"



