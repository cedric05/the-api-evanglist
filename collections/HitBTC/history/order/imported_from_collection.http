#!/usr/bin/env dothttp

# imported from 35240-585a2d49-6eb1-4045-8898-5ab8d446bc39.postman_collection.json

// Get historical orders.

@name("Get Historical Orders")
GET "https://{{baseUrl}}/history/order"
? "symbol"= "<post>"
? "from"= "<>"
? "till"= "<>"
? "limit"= "<>"
? "offset"= "<>"
? "clientOrderId"= "<post>"


// Get historical trades by specified order.

@name("Get Historical Trades By Specified Order")
GET "https://{{baseUrl}}/history/order/:id/trades"



