#!/usr/bin/env dothttp

# imported from 35240-22a10f2d-8624-488d-95c0-85a6c45216c5.postman_collection.json

// Retrieve the thing with the specified id.n

@name("Get a thing")
GET "https://{{baseUrl}}/thing/types/:thingTypeId/things/:thingId"


// Retrieve the current state of the thing with the specified id.n

@name("Get the state for the thing with the specified id")
GET "https://{{baseUrl}}/thing/types/:thingTypeId/things/:thingId/state/:logicalInterfaceId"



