@name("list Group Targets For Role")
GET "https://{{baseUrl}}/api/v1/users/:userId/roles/:roleId/targets/groups"
? "after"= "<string>"
? "limit"= "<integer>"


@name("remove Group Target From Role")
DELETE "https://{{baseUrl}}/api/v1/users/:userId/roles/:roleId/targets/groups/:groupId"



