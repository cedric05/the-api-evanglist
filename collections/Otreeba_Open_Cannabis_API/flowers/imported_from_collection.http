// Returns a paginated list of flowers.

@name("Get a list of all current flowers.")
GET "https://{{baseUrl}}/flowers"
? "page"= "<>"
? "count"= "<get>"
? "sort"= "<>"


// Returns a single flower.

@name("Find flower by Open Cannabis Product Code (OCPC).")
GET "https://{{baseUrl}}/flowers/:ocpc"



