#!/usr/bin/env dothttp

# imported from 35240-62c6cc63-de15-4694-9302-08ed4981816f.postman_collection.json

// Returns a paginated list of strains.

@name("Get a list of all current strains.")
GET "https://{{baseUrl}}/strains"
? "page"= "<>"
? "count"= "<get>"
? "sort"= "<>"


// Returns a single strain.

@name("Find strain by Open Cannabis Product Code (OCPC).")
GET "https://{{baseUrl}}/strains/:ocpc"



