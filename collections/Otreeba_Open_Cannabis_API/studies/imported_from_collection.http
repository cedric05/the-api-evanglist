#!/usr/bin/env dothttp

# imported from 35240-62c6cc63-de15-4694-9302-08ed4981816f.postman_collection.json

// Returns a paginated list of studies.

@name("Get a list of all current studies.")
GET "https://{{baseUrl}}/studies"
? "page"= "<>"
? "count"= "<get>"
? "sort"= "<>"


// Returns a single study.

@name("Find study by DOI, PubMed ID, or slug.")
GET "https://{{baseUrl}}/studies/:identifierType/:identifier"



