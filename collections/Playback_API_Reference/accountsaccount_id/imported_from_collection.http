#!/usr/bin/env dothttp

# imported from 35240-b5d01f46-c1f0-4f14-b0cc-3bece0333256.postman_collection.json

// Gets a playlist object for an account, based on playlist ID or reference ID.   

@name("Get Playlist by ID or Reference ID")
GET "https://{{baseUrl}}/accounts/:account_id/playlists/:playlist_id"
? "limit"= "<number>"
? "offset"= "<number>"
? "ad_config_id"= "<string>"



