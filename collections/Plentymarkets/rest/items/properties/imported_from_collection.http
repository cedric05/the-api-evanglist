#!/usr/bin/env dothttp

# imported from 35240-5af4ed67-4654-4363-a86d-3c7bcff23c51.postman_collection.json

// Lists all properties.

@name("List properties")
GET "https://{{baseUrl}}/rest/items/properties"
? "with"= "<>"
? "updatedAt"= "<post>"
? "groupId"= "<>"


// Deletes a property. The ID of the property must be specified.

@name("Delete a property")
DELETE "https://{{baseUrl}}/rest/items/properties/:id"



