#!/usr/bin/env dothttp

# imported from 35240-7d7a5fc9-3f9b-4b0b-ae23-71c4fb27e02a.postman_collection.json

@name("Delete List")
DELETE "https://{{baseUrl}}/lists/:list_id"


@name("Get all messages that was sent to the given list")
GET "https://{{baseUrl}}/lists/:list_id/messages"
? "type"= "<string>"


// Get the contacts that are a part of the given list

@name("Get the contacts that are a part of the given list...")
GET "https://{{baseUrl}}/lists/:list_id/contacts"
? "type"= "<string>"



