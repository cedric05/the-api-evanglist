#!/usr/bin/env dothttp

# imported from 35240-ac6561cd-0334-4ee5-8280-57b4086d4c3d.postman_collection.json

// By default, Issue Comments are ordered by ascending ID.

@name("List issue comments for a repository")
GET "https://{{baseUrl}}/repos/:owner/:repo/issues/comments"
? "sort"= "created"
? "direction"= "<string>"
? "since"= "<string>"
? "per_page"= "30"
? "page"= "1"



