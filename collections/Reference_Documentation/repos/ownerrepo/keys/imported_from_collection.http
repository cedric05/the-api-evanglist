#!/usr/bin/env dothttp

# imported from 35240-ac6561cd-0334-4ee5-8280-57b4086d4c3d.postman_collection.json

@name("List deploy keys")
GET "https://{{baseUrl}}/repos/:owner/:repo/keys"
? "per_page"= "30"
? "page"= "1"


// You can create a read-only deploy key.

@name("Create a deploy key")
POST "https://{{baseUrl}}/repos/:owner/:repo/keys"
"Content-Type": "application/json"
json({
    "key": "<string>",
    "title": "<string>",
    "read_only": "<boolean>"
})



