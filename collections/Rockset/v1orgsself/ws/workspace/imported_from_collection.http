#!/usr/bin/env dothttp

# imported from 35240-97c6871f-8a54-42bd-a4f1-a3ada62aa1b0.postman_collection.json

// Get information about a single workspace.

@name("Get Workspace")
GET "https://{{baseUrl}}/v1/orgs/self/ws/:workspace"


// Remove a workspace.

@name("Delete Workspace")
DELETE "https://{{baseUrl}}/v1/orgs/self/ws/:workspace"


// List workspaces under given workspace.

@name("List Workspaces in Workspace")
GET "https://{{baseUrl}}/v1/orgs/self/ws/:workspace/ws"



