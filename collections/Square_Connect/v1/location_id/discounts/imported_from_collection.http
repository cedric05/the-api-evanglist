// Lists all of a location's discounts.

@name("Get Location Discounts")
GET "https://{{baseUrl}}/v1/:location_id/discounts"


// Deletes an existing discount.

@name("Delete Location Discounts")
DELETE "https://{{baseUrl}}/v1/:location_id/discounts/:discount_id"



