#!/usr/bin/env dothttp

# imported from 35240-6b84cc71-ce77-48b8-babd-323eb8d670bd.postman_collection.json

// This service returns substances by CAS numbers in bulk.

@name("Get substances by a list of CAS numbers.")
GET "https://{{baseUrl}}/substances/cas"
? "casList"= "<string>"
? "excludeSynonyms"= "<string>"
? "includeOtherCas"= "<string>"
? "includeOtherTsn"= "<string>"
? "listAcronym"= "<string>"
? "pageNum"= "<string>"
? "pageSize"= "<string>"
? "qualifier"= "exact"


// This service returns substances by characteristic.

@name("Get substances by characteristic.")
GET "https://{{baseUrl}}/substances/characteristic/:characteristic"
? "excludeSynonyms"= "<string>"
? "includeOtherCas"= "<string>"
? "includeOtherTsn"= "<string>"
? "pageNum"= "<string>"
? "pageSize"= "<string>"
? "qualifier"= "exact"


