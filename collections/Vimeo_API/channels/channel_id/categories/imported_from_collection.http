#!/usr/bin/env dothttp

# imported from 35240-04551e11-ae48-4759-9d58-fef6a7b17230.postman_collection.json

// This method gets all the categories in the specified channel.

@name("Get all the categories in a channel")
GET "https://{{baseUrl}}/channels/:channel_id/categories"


// This method removes a single category from the specified channel.

@name("Remove a category from a channel")
DELETE "https://{{baseUrl}}/channels/:channel_id/categories/:category"



