#!/usr/bin/env dothttp

# imported from 35240-04551e11-ae48-4759-9d58-fef6a7b17230.postman_collection.json

// This method gets a specific tag from all available tags.

@name("Get a specific tag")
GET "https://{{baseUrl}}/tags/:word"


// This method returns all the videos associated with the specified tag.

@name("Get all the videos with a specific tag")
GET "https://{{baseUrl}}/tags/:word/videos"
? "direction"= "<string>"
? "page"= "<string>"
? "per_page"= "<string>"
? "sort"= "<string>"



