#!/usr/bin/env dothttp

# imported from 35240-04551e11-ae48-4759-9d58-fef6a7b17230.postman_collection.json

// This method returns all the tags associated with a video.

@name("Get all the tags of a video")
GET "https://{{baseUrl}}/videos/:video_id/tags"


// This method removes the specified tag from a video.

@name("Remove a tag from a video")
DELETE "https://{{baseUrl}}/videos/:video_id/tags/:word"



