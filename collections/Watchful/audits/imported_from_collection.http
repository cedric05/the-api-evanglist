#!/usr/bin/env dothttp

# imported from 35240-dd3bb79e-7d36-49f2-9d90-ac848a0b6d74.postman_collection.json

// Returns a list of audits

@name("Get A List Of Audits")
GET "https://{{baseUrl}}/audits"
? "limit"= "<>"
? "limitstart"= "<>"
? "order"= "<>"


// Returns a list of fields

@name("Get The List Of Fields")
GET "https://{{baseUrl}}/audits/metadata"


// Delete a specific audit.

@name("Delete A Specific Audit")
DELETE "https://{{baseUrl}}/audits/:id"



